
struct Polynomial(coefficients:List)
{
    eval(this:Polynomial, x:Num)
    {
        evaluator(a:Num, b:List.EnumeratedElement):Num
        {
            return = a.add(b.val.mul(x.pow(b.idx)))
        }
        return = this.coefficients.enumerate.fold(0, evaluator)
    }
}


